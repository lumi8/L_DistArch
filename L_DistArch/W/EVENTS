	ThisProject.Variables("logfiltrocell%").Value = ""
	ThisProject.Variables("filtrodesc%").Value = ""
	
	'Refresh Filter List
	FilterListRefresh ThisProject.Path & "\C\" & "logfilter.ini"
	End Sub
	
	Private Sub NoLogInOut_Click()
	    If NoLogInOut.Value = True Then
	        ThisMimic.Graphics.Item("Log1").SetEventMask ResetEvMaskFlags(OpActFlags) ' Nascondi login/logout
	    Else
	        ThisMimic.Graphics.Item("Log1").SetEventMask SetEvMaskFlags(OpActFlags) ' Mostra login/logout
	    End If
	    ThisMimic.Graphics.Item("Log1").Execute
	End Sub
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
