	'-----Gestione filtro sulla cella
	
	Private Sub FiltraCella_Click()
	    Dim filtro As String
	    Dim FiltroCell As String
	    Dim FiltroDesc As String
	        
	    FiltroCell = ThisProject.Variables("logfiltrocell%").Value                                        'Prelevo filtro impostato per cella da usare per #A8
	    FiltroDesc = ThisProject.Variables("filtrodesc%").Value
	    If Len(FiltroCell) = 0 And Len(FiltroDesc) = 0 Then
	        Exit Sub
	    End If
	    If Len(FiltroCell) > 0 And Len(FiltroDesc) = 0 Then filtro = "=(#A8 INC " & FiltroCell & ")"        'Filtro composto solo Cella
	    If Len(FiltroDesc) > 0 And Len(FiltroCell) = 0 Then filtro = "=(#T INC " & FiltroDesc & ")"         'Filtro composto solo Descrizione
	    If Len(FiltroCell) > 0 And Len(FiltroDesc) > 0 Then
	        filtro = "=(#A8 INC " & FiltroCell & ")&&(#T INC " & FiltroDesc & ")"                           'Filtro composto per Cella e Descrizione
	    End If
	    Me.txtCellDescFilter.Text = filtro
	    
	    If filtro = "" Then
	        Log1.BasicChangeAttFilter ""
	        Me.FiltroCell.BackColorPrimary = RGB(255, 255, 255)
	        Me.FiltroDescr.BackColorPrimary = RGB(255, 255, 255)
	    Else
	        Log1.BasicChangeAttFilter filtro
	        Me.FiltroCell.BackColorPrimary = RGB(255, 255, 0)
	        Me.FiltroDescr.BackColorPrimary = RGB(255, 255, 0)
	    End If
	    
	End Sub
	
	
	Private Sub Log1_OnLineSelect(ByVal cx As Integer, ByVal cy As Integer, ByVal handle As String, ByVal selected As Boolean)
	
	End Sub
	
	Private Sub ResetFiltroCella_Click()
	    ThisProject.Variables("logfiltrocell%").Value = ""
	    Me.FiltroCell.BackColorPrimary = RGB(255, 255, 255)
	    ThisProject.Variables("filtrodesc%").Value = ""
	    Me.FiltroDescr.BackColorPrimary = RGB(255, 255, 255)
	    Log1.BasicChangeAttFilter ""
	End Sub
	
	Private Sub ComboFilter_Click()
	    Dim Rec As String
	    Dim NC As Long
	    
	    If ThisMimic.Graphics.Item("ComboFilter").Text = "" Then
	        ThisMimic.Graphics.Item("ComboDom").Text = ""
	        ThisMimic.Graphics.Item("Combonat").Text = ""
	        ThisMimic.Graphics.Item("ComboLivMin").Text = ""
	        ThisMimic.Graphics.Item("ComboLivMax").Text = ""
	        ThisMimic.Graphics.Item("CheckNoAckOff").Value = False
	        ThisMimic.Graphics.Item("CheckAckOn").Value = False
	        ThisMimic.Graphics.Item("CheckNoAckOn").Value = False
	        ThisProject.Variables("logfiltrocell%").Value = ""
	        ThisProject.Variables("filtrodesc%").Value = ""
	        CheckNoAckOn_Click
	        
	    Else
	        Rec = String(255, 0)
	        NC = GetPrivateProfileString(ThisMimic.Graphics.Item("ComboFilter").Text, "Domain", "", Rec, 255, ThisProject.Path & "\C\" & "logfilter.ini")
	        ThisMimic.Graphics.Item("ComboDom").Text = Rec
	        NC = GetPrivateProfileString(ThisMimic.Graphics.Item("ComboFilter").Text, "Nature", "", Rec, 255, ThisProject.Path & "\C\" & "logfilter.ini")
	        ThisMimic.Graphics.Item("Combonat").Text = Rec
	        NC = GetPrivateProfileString(ThisMimic.Graphics.Item("ComboFilter").Text, "LivMin", "", Rec, 255, ThisProject.Path & "\C\" & "logfilter.ini")
	        ThisMimic.Graphics.Item("ComboLivMin").Text = Rec
	        NC = GetPrivateProfileString(ThisMimic.Graphics.Item("ComboFilter").Text, "LivMax", "", Rec, 255, ThisProject.Path & "\C\" & "logfilter.ini")
	        ThisMimic.Graphics.Item("ComboLivMax").Text = Rec
	        NC = GetPrivateProfileString(ThisMimic.Graphics.Item("ComboFilter").Text, "NoAckOff", "", Rec, 255, ThisProject.Path & "\C\" & "logfilter.ini")
	        ThisMimic.Graphics.Item("CheckNoAckOff").Value = Rec
	        NC = GetPrivateProfileString(ThisMimic.Graphics.Item("ComboFilter").Text, "AckOn", "", Rec, 255, ThisProject.Path & "\C\" & "logfilter.ini")
	        ThisMimic.Graphics.Item("CheckAckOn").Value = Rec
	        NC = GetPrivateProfileString(ThisMimic.Graphics.Item("ComboFilter").Text, "NoAckOn", "", Rec, 255, ThisProject.Path & "\C\" & "logfilter.ini")
	        ThisMimic.Graphics.Item("CheckNoAckon").Value = Rec
	        
	        NC = GetPrivateProfileString(ThisMimic.Graphics.Item("ComboFilter").Text, "Cell", "", Rec, 255, ThisProject.Path & "\C\" & "logfilter.ini")
	        ThisProject.Variables("logfiltrocell%").Value = Rec
	        NC = GetPrivateProfileString(ThisMimic.Graphics.Item("ComboFilter").Text, "Desc", "", Rec, 255, ThisProject.Path & "\C\" & "logfilter.ini")
	        ThisProject.Variables("filtrodesc%").Value = Rec
	        If Len(ThisProject.Variables("filtrodesc%").Value) + Len(ThisProject.Variables("logfiltrocell%").Value) > 0 Then FiltraCella_Click
	        
	        CheckNoAckOn_Click
	    End If
	    
	End Sub
	
	Private Sub ComboLivMax_Change()
	    Dim lvlmax As Integer
	    Dim lvlMin As Integer
	
	    If ThisMimic.Graphics.Item("ComboLivMax").Text = "" Then
	        lvlmax = 29
	    Else
	        lvlmax = CInt(Left(ThisMimic.Graphics.Item("ComboLivMax").Text, 2))
	    End If
	
	
	    If ThisMimic.Graphics.Item("ComboLivMin").Text = "" Then
	        lvlMin = 0
	    Else
	        lvlMin = CInt(Left(ThisMimic.Graphics.Item("ComboLivMin").Text, 2))
	    End If
	
	    ThisMimic.Graphics.Item("Log1").SetMinMax lvlMin, lvlmax
	    ThisMimic.Graphics.Item("Log1").Execute
	End Sub
	
	Private Sub ComboLivMin_Change()
	    ComboLivMax_Change
	End Sub
	
	Private Sub ComboNat_Change()
	    Dim Nature As String
	
	    Nature = ThisMimic.Graphics.Item("ComboNat").Text
	    ThisMimic.Graphics.Item("Log1").BasicChangeNature Nature
	End Sub
	
	'Private Sub FiltraDescrizione_Click()
	'    Dim filtro As String
	'    ' Se per caso non Š stata inizializzata
	'    If CurrentEvMask = 0 Then
