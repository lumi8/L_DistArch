VBA_HOSTPROJECT,BEGIN
	Option Explicit
	
	'// Initial declaration of DLL
	 'read
	   Private Declare Function GetPrivateProfileString Lib "kernel32" Alias "GetPrivateProfileStringA" _
	                 (ByVal lpApplicationName As String, _
	                   ByVal lpKeyName As String, _
	                   ByVal lpDefault As String, _
	                   ByVal lpReturnedString As String, _
	                   ByVal nSize As Long, _
	                   ByVal lpFileName As String) As Long
	                   
	   'write & delete
	   Private Declare Function WritePrivateProfileString Lib "kernel32" Alias "WritePrivateProfileStringA" _
	                 (ByVal lpApplicationName As String, _
	                   ByVal lpKeyName As String, _
	                   ByVal lpString As String, _
	                   ByVal lpFileName As String) As Long
	                   
	    'list
	    Private Declare Function GetPrivateProfileSectionNames Lib "kernel32.dll" Alias "GetPrivateProfileSectionNamesA" _
	                   (ByVal lpszReturnBuffer As String, _
	                   ByVal nSize As Long, _
	                   ByVal lpFileName As String) As Long
	    
	    Private DisableRefresh As Boolean
	    
	' Da mantenere allineata con le impostazioni filtro dell'oggetto Log
	'                                      Bit0 Bit1 OnAck OnNotAck OffAck OffNotAck|         Allarme inibito e mascherato           | Utente Alm mask/unmask |    Login fail/in/out  |AckAlm |SendReg/txt
	Private Const InitialEvMask As Double = 64 + 512 + 2048 + 4096 + 8192 + 16384 + 2097152 + 4194304 + 8388608 + 16777216 + 33554432 + 67108864 + 134217728 + 1073741824 + 128 + 256 + 65536 + 8 + 16
	Private Const OpActFlags As Double = 1073741824 + 128 + 256 ' Login fail/in/out
	Private CurrentEvMask As Double
	
	Private Function SetEvMaskFlags(flags As Double) As Double
	    CurrentEvMask = CurrentEvMask Or flags
	    SetEvMaskFlags = CurrentEvMask
	End Function
	Private Function ResetEvMaskFlags(flags As Double) As Double
	    CurrentEvMask = CurrentEvMask And Not flags
	    ResetEvMaskFlags = CurrentEvMask
	End Function
	
	
	Private Function GetIniSections(FileName As String) As String()
	Dim strBuffer As String, intLen As Integer
	On Error GoTo ErrHandler
	intLen = -1
	
	Do While (intLen = Len(strBuffer) - 2) Or (intLen = -1)
	
	If strBuffer = vbNullString Then
	strBuffer = Space(256)
	Else
	strBuffer = String(Len(strBuffer) * 2, 0)
	End If
	
	intLen = GetPrivateProfileSectionNames(strBuffer, Len(strBuffer), FileName)
	
	
	Loop
	
	strBuffer = Left$(strBuffer, intLen)
	
	GetIniSections = Split(strBuffer, vbNullChar)
	
	ReDim Preserve GetIniSections(UBound(GetIniSections) - 1) As String
	ErrHandler:
	
	End Function
	
	
	
	Private Sub FilterListRefresh(FileName As String)
	Dim Sections() As String
	Dim Section As Variant
	   
	On Error GoTo ErrHandler
	    ThisMimic.Graphics.Item("ComboFilter").Clear
	    ThisMimic.Graphics.Item("ComboFilter").addItem ("")
	
	    Sections = GetIniSections(FileName)
	
	For Each Section In Sections
	    ThisMimic.Graphics.Item("ComboFilter").addItem (Section)
	Next
	ErrHandler:
	
	End Sub
	
	Private Sub DeleteKey(FileName As String, SectionName As String)
	
	      
	    WritePrivateProfileString SectionName, vbNullString, vbNullString, FileName
	
	
	End Sub
	
	Private Sub WriteProfile(SectionName As String, KeyName As String, KeyValue As String, FileName As String)
	
	    WritePrivateProfileString SectionName, KeyName, KeyValue, FileName
	
	End Sub
	
	
	Private Sub CheckAckOn_Click()
	
	CheckNoAckOn_Click
	
	End Sub
	
	Private Sub CheckNoAckOff_Click()
	
	CheckNoAckOn_Click
	
	
	End Sub
	
	Private Sub CheckNoAckOn_Click()
	
	' Se per caso non Š stata inizializzata
	If CurrentEvMask = 0 Then
	    CurrentEvMask = InitialEvMask
	End If
	
	If CheckNoAckOn.Value = False And CheckNoAckOff.Value = False And CheckAckOn.Value = False Then 'Nothing
	
	'ThisMimic.Graphics.Item("Log1").SetEventMask 22528
	ThisMimic.Graphics.Item("Log1").SetEventMask SetEvMaskFlags(InitialEvMask) ' AS 14/11/2014
	ThisMimic.Graphics.Item("Log1").Execute
	
	End If
	
	If CheckNoAckOn.Value = True And CheckNoAckOff.Value = False And CheckAckOn.Value = False Then 'NoAckOn
	
	ThisMimic.Graphics.Item("Log1").SetEventMask 4096
	ThisMimic.Graphics.Item("Log1").Execute
	
	End If
	
	If CheckNoAckOn.Value = False And CheckNoAckOff.Value = True And CheckAckOn.Value = False Then 'NoAckOff
	
	ThisMimic.Graphics.Item("Log1").SetEventMask 16384
	ThisMimic.Graphics.Item("Log1").Execute
	
	End If
	
	If CheckNoAckOn.Value = False And CheckNoAckOff.Value = False And CheckAckOn.Value = True Then 'AckOn
	
	ThisMimic.Graphics.Item("Log1").SetEventMask 2048
	ThisMimic.Graphics.Item("Log1").Execute
	
	End If
	
	If CheckNoAckOn.Value = True And CheckNoAckOff.Value = True And CheckAckOn.Value = False Then 'NoAckOn & NoAckOff
	
	ThisMimic.Graphics.Item("Log1").SetEventMask 20480
	ThisMimic.Graphics.Item("Log1").Execute
	
	End If
	
	If CheckNoAckOn.Value = True And CheckNoAckOff.Value = False And CheckAckOn.Value = True Then 'NoAckOn & AckOn
	
	ThisMimic.Graphics.Item("Log1").SetEventMask 6144
	ThisMimic.Graphics.Item("Log1").Execute
	
	End If
	
	If CheckNoAckOn.Value = False And CheckNoAckOff.Value = True And CheckAckOn.Value = True Then 'NoAckOff & AckOn
	
	ThisMimic.Graphics.Item("Log1").SetEventMask 18432
	ThisMimic.Graphics.Item("Log1").Execute
	
	End If
	
	If CheckNoAckOn.Value = True And CheckNoAckOff.Value = True And CheckAckOn.Value = True Then 'NoAckOn & NoAckOff & AckOn
	
	ThisMimic.Graphics.Item("Log1").SetEventMask 22528
	ThisMimic.Graphics.Item("Log1").Execute
	
	End If
	
	End Sub
	
	Private Sub cmdElimina_Click()
	Dim Profile As String
	Profile = ThisMimic.Graphics.Item("ComboFilter").Text
	    
	    DeleteKey ThisProject.Path & "\C\" & "logfilter.ini", Profile
	    FilterListRefresh ThisProject.Path & "\C\" & "logfilter.ini"
	
	End Sub
	
	Private Sub cmdResetAllFilters_Click()
	
	'Resetta filtri per eventi
	ThisMimic.Graphics.Item("ComboDom").Text = ""               'Reset filtro DOMINIO
	ThisMimic.Graphics.Item("Log1").BasicChangeDomain ""
	'ThisMimic.Graphics.Item("ComboDueColonne").Text = ""        'Reset filtro NATURA
	ThisMimic.Graphics.Item("Log1").BasicChangeNature ""
	ResetFiltroCella_Click                                      'Reset filtro per CELLA
	'ResetFiltroDescrizione_Click                                'Reset filtro per DESCRIZIONE
	
	ThisMimic.Graphics.Item("ComboLivMin").Text = ""
	ThisMimic.Graphics.Item("ComboLivMax").Text = ""
	ThisMimic.Graphics.Item("CheckNoAckOff").Value = True
	ThisMimic.Graphics.Item("CheckAckOn").Value = True
	ThisMimic.Graphics.Item("CheckNoAckOn").Value = True
	
	ThisMimic.Graphics.Item("ComboFilter").Text = ""
	
	End Sub
	
	Private Sub cmdSalva_Click()
	Dim Profile As String
	Profile = ThisMimic.Graphics.Item("ComboFilter").Text
	
	    WriteProfile Profile, "Domain", ThisMimic.Graphics.Item("ComboDom").Text, ThisProject.Path & "\C\" & "logfilter.ini"
	    WriteProfile Profile, "Nature", ThisMimic.Graphics.Item("Combonat").Text, ThisProject.Path & "\C\" & "logfilter.ini"
	    
	    WriteProfile Profile, "LivMin", ThisMimic.Graphics.Item("ComboLivMin").Text, ThisProject.Path & "\C\" & "logfilter.ini"
	    WriteProfile Profile, "LivMax", ThisMimic.Graphics.Item("ComboLivMax").Text, ThisProject.Path & "\C\" & "logfilter.ini"
	    
	    WriteProfile Profile, "AckOn", ThisMimic.Graphics.Item("CheckAckOn").Value, ThisProject.Path & "\C\" & "logfilter.ini"
	    WriteProfile Profile, "NoAckOn", ThisMimic.Graphics.Item("CheckNoAckOn").Value, ThisProject.Path & "\C\" & "logfilter.ini"
	    WriteProfile Profile, "NoAckOff", ThisMimic.Graphics.Item("CheckNoAckOff").Value, ThisProject.Path & "\C\" & "logfilter.ini"
	
	    WriteProfile Profile, "Cell", ThisProject.Variables("logfiltrocell%").Value, ThisProject.Path & "\C\" & "logfilter.ini"
	    WriteProfile Profile, "Desc", ThisProject.Variables("filtrodesc%").Value, ThisProject.Path & "\C\" & "logfilter.ini"
	
	    FilterListRefresh ThisProject.Path & "\C\" & "logfilter.ini"
	
	End Sub
	
